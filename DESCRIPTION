Package: rshift
Type: Package
Title: Paleoecology Functions for Regime Shift Analysis
Version: 2.2.0
Authors@R: c(person("Alex H.", "Room", 
                    role = c("aut", "cre", "cph"), 
                    email = "alex.room@btinternet.com"),
             person("Felipe", "Franco-Gaviria", 
                    role = c("ctb", "fnd")),
             person("Dunia H.", "Urrego", 
