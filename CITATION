year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{rshift}: paleoecology and regime shift analysis",
         author = c(person("Alex", "Pinheiro"),
                    person("Felipe", "Franco-Gaviria"),
                    person("Dunia", "Urrego"),
         year = year,
         url = "https://CRAN.R-project.org/package=rshift")

